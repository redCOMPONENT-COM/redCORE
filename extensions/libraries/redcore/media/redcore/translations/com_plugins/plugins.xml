      <field name="enabled" type="state" translate="1" alwaysFallback="true" filter="RAW" description="">Enabled</field>
      <filter><![CDATA[o.type = 'plugin']]></filter>
    </table>
    <component>
      <editForm admin="true" option="com_plugins" view="plugin" layout="edit" identifier="extension_id" showbutton="true" htmlposition=".btn-toolbar:first" checkoriginalid="false"/>
    </component>
  </reference>
