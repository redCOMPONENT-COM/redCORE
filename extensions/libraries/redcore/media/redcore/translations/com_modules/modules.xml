  <authorEmail>email@redweb.dk</authorEmail>
  <authorUrl>www.redweb.dk</authorUrl>
  <version>1.10.6</version>
  <description>Translation definition for Joomla Module Management</description>
  <reference>
    <table name="modules">
      <field name="id" type="referenceid" translate="0" alwaysFallback="false" filter="RAW" description="">ID</field>
      <field name="title" type="titletext" translate="1" alwaysFallback="false" filter="RAW" description="">Title</field>
      <field name="module" type="titletext" translate="0" alwaysFallback="false" filter="RAW" description="">Module</field>
      <field name="content" type="htmltext" translate="1" alwaysFallback="false" filter="RAW" description="">Content</field>
      <field name="client_id" type="text" translate="0" alwaysFallback="false" filter="RAW" description="">Module</field>
      <field name="params" type="params" translate="1" alwaysFallback="true" filter="RAW" description="" formname="advanced" formname25="module" fieldsname="params">Parameters</field>
      <field name="published" type="state" translate="1" alwaysFallback="true" filter="RAW" description="">State</field>
      <filter><![CDATA[o.client_id = 0]]></filter>
    </table>
    <component>
      <editForm admin="true" option="com_modules" view="module" layout="edit" identifier="id" showbutton="true" htmlposition=".btn-toolbar:first" checkoriginalid="false"/>
    </component>
  </reference>
</contentelement>
