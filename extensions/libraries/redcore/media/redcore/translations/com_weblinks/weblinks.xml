    <description>Translation definition for Joomla Weblinks component</description>
    <reference>
        <table name="weblinks">
            <field name="id" type="referenceid" translate="0" alwaysFallback="false" filter="RAW" description="">ID</field>
            <field name="title" type="titletext" translate="1" alwaysFallback="false" filter="RAW" description="">Title</field>
            <field name="description" type="textarea" translate="1" alwaysFallback="false" filter="RAW" description="">Description</field>
            <field name="url" type="text" translate="1" alwaysFallback="false" filter="RAW" description="" maxlength="1000">URL</field>
            <field name="state" type="state" translate="1" alwaysFallback="true" filter="RAW" description="">State</field>
            <filter><![CDATA[]]></filter>
        </table>
        <component>
            <editForm admin="true" option="com_weblinks" view="weblink" layout="edit" identifier="id" showbutton="true" htmlposition=".btn-toolbar:first" checkoriginalid="false"/>
        </component>
    </reference>
</contentelement>
