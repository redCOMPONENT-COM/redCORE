<?xml version="1.0"?>
<contentelement type="contentelement">
  <name>Contact - details</name>
  <author>redCOMPONENT</author>
  <copyright>Copyright (C) 2008 - 2016 redCOMPONENT.com. All rights reserved.</copyright>
  <version>1.8.3</version>
  <description>Translation definition for Joomla Contact component</description>
  <reference>
    <table name="contact_details">
      <field name="id" type="referenceid" translate="0" alwaysFallback="false" filter="RAW" description="">ID</field>
      <field name="name" type="titletext" translate="1" alwaysFallback="false" filter="RAW" description="">Name</field>
      <field name="alias" type="text" translate="1" alwaysFallback="false" filter="RAW" description="" posthandler="filterTitle">Alias</field>
      <field name="con_position" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Position</field>
      <field name="email_to" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Email Address</field>
      <field name="address" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Street Address</field>
      <field name="suburb" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Town/Suburb</field>
      <field name="state" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">State/County</field>
      <field name="country" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Country</field>
      <field name="postcode" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Postcode</field>
      <field name="telephone" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Telefon</field>
      <field name="fax" type="text" translate="1" alwaysFallback="false" filter="RAW" description="">Fax</field>
      <field name="misc" type="textarea" translate="1" alwaysFallback="false" filter="RAW" description="">Miscellaneous Info</field>
      <field name="params" type="params" translate="1" alwaysFallback="true" filter="RAW" description="">Parameters</field>
      <field name="published" type="state" translate="1" alwaysFallback="true" filter="RAW" description="">State</field>
      <filter><![CDATA[]]></filter>
    </table>
    <component>
      <editForm admin="true" option="com_contact" view="contact" layout="edit" identifier="id" showbutton="true" htmlposition=".btn-toolbar:first" checkoriginalid="false"/>
    </component>
  </reference>
</contentelement>
